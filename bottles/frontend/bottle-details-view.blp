using Gtk 4.0;
using Adw 1;

template $BottleDetailsView: Adw.NavigationPage {
  title: _("Details");
  tag: "details";

  child: Box {
    orientation: vertical;

    Adw.HeaderBar sidebar_headerbar {
      title-widget: Adw.WindowTitle sidebar_title {
        title: _("Details");
      };

      [end]
      Box default_actions {}
    }

    Box default_view {}
  };
}
