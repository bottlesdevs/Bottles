<?xml version="1.0" encoding="UTF-8"?>
<interface>
<requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <object class="GtkPopover" id="pop_browse">
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">3</property>
        <child>
          <object class="GtkModelButton" id="btn_explorer">
            <property name="text" translatable="yes">Open with explorer</property>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkPopover" id="pop_context">
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">3</property>
        <child>
          <object class="GtkModelButton" id="btn_flatpak_doc">
            <property name="text" translatable="yes">Troubleshooting</property>
          </object>
        </child>
        <child>
          <object class="GtkSeparator"/>
        </child>
        <child>
          <object class="GtkModelButton" id="btn_duplicate">
            <property name="text" translatable="yes">Duplicate bottle</property>
          </object>
        </child>
        <child>
          <object class="GtkModelButton" id="btn_backup_full">
            <property name="tooltip-text" translatable="yes">This is the complete archive of your bottle, including personal files.</property>
            <property name="text" translatable="yes">Full backup</property>
          </object>
        </child>
        <child>
          <object class="GtkModelButton" id="btn_backup_config">
            <property name="tooltip-text" translatable="yes">This is just the bottle configuration, it&apos;s perfect if you want to create a new one but without personal files.</property>
            <property name="text" translatable="yes">Export configuration</property>
          </object>
        </child>
        <child>
          <object class="GtkSeparator"/>
        </child>
        <child>
          <object class="GtkModelButton" id="btn_delete">
            <property name="text" translatable="yes">Delete bottle</property>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkBox" id="actions">
    <property name="spacing">8</property>
    <child>
      <object class="GtkMenuButton">
        <property name="focus-on-click">0</property>
        <property name="valign">center</property>
        <property name="popover">pop_context</property>
        <child>
          <object class="GtkImage">
            <property name="icon-name">view-more-symbolic</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <placeholder/>
    </child>
    <style>
      <class name="linked"/>
    </style>
  </object>
  <object class="GtkPopover" id="pop_power">
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">3</property>
        <child>
          <object class="GtkModelButton" id="btn_killall">
            <property name="text" translatable="yes">Kill all processes</property>
          </object>
        </child>
        <child>
          <object class="GtkModelButton" id="btn_shutdown">
            <property name="tooltip-text" translatable="yes">Simulate a Windows system shutdown.</property>
            <property name="text" translatable="yes">Shutdown</property>
          </object>
        </child>
        <child>
          <object class="GtkModelButton" id="btn_reboot">
            <property name="tooltip-text" translatable="yes">Simulate a Windows system reboot.</property>
            <property name="text" translatable="yes">Reboot</property>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkBox" id="actions">
    <property name="visible">True</property>
    <property name="can-focus">False</property>
    <property name="spacing">8</property>
    <child>
      <object class="GtkMenuButton">
        <property name="visible">True</property>
        <property name="can-focus">True</property>
        <property name="focus-on-click">False</property>
        <property name="receives-default">True</property>
        <property name="valign">center</property>
        <property name="popover">pop_context</property>
        <child>
          <object class="GtkImage">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="icon-name">view-more-symbolic</property>
          </object>
        </child>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="fill">True</property>
        <property name="pack-type">end</property>
        <property name="position">0</property>
      </packing>
    </child>
    <child>
      <object class="GtkMenuButton">
        <property name="visible">True</property>
        <property name="can-focus">True</property>
        <property name="focus-on-click">False</property>
        <property name="receives-default">True</property>
        <property name="valign">center</property>
        <property name="draw-indicator">True</property>
        <property name="popover">pop_power</property>
        <child>
          <object class="GtkBox">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="spacing">2</property>
            <child>
              <object class="GtkImage">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="icon-name">system-shutdown-symbolic</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkImage">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="icon-name">pan-down-symbolic</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">2</property>
              </packing>
            </child>
          </object>
        </child>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="fill">True</property>
        <property name="pack-type">end</property>
        <property name="position">1</property>
      </packing>
    </child>
    <child>
      <placeholder/>
    </child>
  </object>
  <object class="GtkPopover" id="pop_run">
    <child>
      <object class="GtkBox" id="box_run_extra">
        <property name="orientation">vertical</property>
        <property name="spacing">3</property>
        <child>
          <object class="GtkModelButton" id="btn_run_args">
            <property name="text" translatable="yes">Run with arguments</property>
          </object>
        </child>
        <child>
          <object class="GtkCheckButton" id="check_move_file">
            <property name="label" translatable="yes">Move inside the sandbox</property>
          </object>
        </child>
        <child>
          <object class="GtkCheckButton" id="check_terminal">
            <property name="label" translatable="yes">Launch with terminal</property>
            <property name="halign">start</property>
          </object>
        </child>
        <child>
          <object class="GtkSeparator" id="extra_separator"/>
        </child>
      </object>
    </child>
  </object>
  <template class="DetailsBottle" parent="GtkScrolledWindow">
    <child>
      <object class="GtkViewport">
        <child>
          <object class="AdwClamp">
            <child>
              <object class="GtkBox">
                <property name="margin-end">10</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkBox">
                    <property name="spacing">5</property>
                    <child>
                      <object class="GtkEntry" id="entry_name">
                        <property name="halign">start</property>
                        <property name="editable">0</property>
                        <property name="max-length">20</property>
                        <property name="has-frame">0</property>
                        <property name="text" translatable="yes">Bottle name</property>
                        
                        
                        <property name="placeholder-text" translatable="yes">Type a name for your bottle</property>
                        <style>
                          <class name="borderless"/>
                          <class name="heading_1"/>
                          <class name="no-padding"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkBox">
                    <property name="spacing">5</property>
                    <child>
                      <object class="GtkLabel" id="label_arch">
                        <property name="halign">start</property>
                        <property name="label" translatable="yes">Win64</property>
                        <property name="wrap">1</property>
                        <property name="wrap-mode">word-char</property>
                        <attributes>
                          <attribute name="font-desc" value="Cantarell Bold 10"></attribute>
                          <attribute name="weight" value="semibold"></attribute>
                        </attributes>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="label_environment">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label" translatable="yes">Environment</property>
                        <attributes>
                          <attribute name="font-desc" value="Cantarell 10"></attribute>
                          <attribute name="weight" value="normal"></attribute>
                        </attributes>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="label_runner">
                        <property name="halign">start</property>
                        <property name="label" translatable="yes">Vaniglia-6.21-x86_64</property>
                        <property name="wrap">1</property>
                        <property name="wrap-mode">word-char</property>
                        <attributes>
                          <attribute name="font-desc" value="Cantarell Bold 10"></attribute>
                          <attribute name="weight" value="normal"></attribute>
                        </attributes>
                      </object>
                    </child>
                    <child>
                      <object class="GtkGrid" id="grid_versioning">
                        <property name="tooltip-text" translatable="yes">Versioning enabled for this bottle</property>
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <child>
                          <object class="GtkImage" id="img_versioning">
                            <property name="tooltip-text" translatable="yes">Versioning is active in this bottle.</property>
                            <property name="halign">center</property>
                            <property name="valign">center</property>
                            <property name="pixel-size">14</property>
                            <property name="icon-name">view-wrapped-symbolic</property>
                            <property name="icon_size">1</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkLabel" id="label_state">
                            <property name="halign">center</property>
                            <property name="valign">center</property>
                            <property name="label" translatable="yes">0</property>
                            <attributes>
                              <attribute name="weight" value="medium"></attribute>
                            </attributes>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwPreferencesGroup" id="group_layers">
                    <property name="title" translatable="yes">Layers</property>
                    <child>
                      <placeholder/>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkBox" id="box_actions">
                    <property name="spacing">5</property>
                    <child>
                      <object class="GtkBox">
                        <child>
                          <object class="GtkButton" id="btn_execute">
                            <property name="label" translatable="yes">Run executable</property>
                            <style>
                              <class name="suggested-action"/>
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkMenuButton">
                            <property name="focus-on-click">0</property>
                            <property name="popover">pop_run</property>
                            <child>
                              <placeholder/>
                            </child>
                            <style>
                              <class name="suggested-action"/>
                            </style>
                          </object>
                        </child>
                        <style>
                          <class name="linked"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <child>
                          <object class="GtkMenuButton">
                            <property name="focus-on-click">0</property>
                            <property name="popover">pop_power</property>
                            <child>
                              <object class="GtkBox">
                                <property name="spacing">2</property>
                                <child>
                                  <object class="GtkImage">
                                    <property name="icon-name">system-shutdown-symbolic</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkImage">
                                    <property name="icon-name">pan-down-symbolic</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkToggleButton" id="btn_rename">
                            <property name="tooltip-text" translatable="yes">Rename bottle</property>
                            <property name="halign">center</property>
                            <property name="valign">center</property>
                            <child>
                              <object class="GtkImage">
                                <property name="icon-name">document-edit-symbolic</property>
                              </object>
                            </child>
                            <style>
                              <class name="heading_1"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkRevealer" id="reveal_progress">
                    <property name="transition-type">slide-up</property>
                    <child>
                      <object class="GtkProgressBar" id="progress_bar">
                        <property name="text" translatable="yes">Moving inside the sandboxâ€¦</property>
                        <property name="show-text">1</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwPreferencesGroup" id="group_programs">
                    <property name="title" translatable="yes">Programs</property>
                    <child>
                      <placeholder/>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwPreferencesGroup" id="group_utilities">
                    <property name="title" translatable="yes">Other</property>
                    <child>
                      <object class="AdwExpanderRow">
                        <property name="title" translatable="yes">Utilities</property>
                        <child>
                          <object class="AdwActionRow" id="row_browse">
                            <property name="title" translatable="yes">Browse C:</property>
                            <property name="subtitle" translatable="yes">Browse internal files.</property>
                            <child>
                              <object class="GtkBox">
                                <property name="spacing">10</property>
                                <child>
                                  <object class="GtkMenuButton">
                                    <property name="focus-on-click">0</property>
                                    <property name="halign">center</property>
                                    <property name="valign">center</property>
                                    <property name="popover">pop_browse</property>
                                    <child>
                                      <object class="GtkImage">
                                        <property name="icon-name">view-more-symbolic</property>
                                      </object>
                                    </child>
                                    <style>
                                      <class name="image-button"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkSeparator"/>
                                </child>
                                <child>
                                  <object class="GtkButton" id="btn_browse">
                                    <property name="halign">center</property>
                                    <property name="valign">center</property>
                                    <child>
                                      <object class="GtkImage">
                                        <property name="icon-name">system-run-symbolic</property>
                                      </object>
                                    </child>
                                    <style>
                                      <class name="btn_undecorated"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow">
                            <property name="title" translatable="yes">Command line</property>
                            <property name="subtitle" translatable="yes">Run commands inside the Bottle.</property>
                            <child>
                              <object class="GtkBox">
                                <property name="spacing">10</property>
                                <child>
                                  <object class="GtkButton" id="btn_cmd">
                                    <property name="halign">center</property>
                                    <property name="valign">center</property>
                                    <child>
                                      <object class="GtkImage">
                                        <property name="icon-name">system-run-symbolic</property>
                                      </object>
                                    </child>
                                    <style>
                                      <class name="btn_undecorated"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow" id="row_regedit">
                            <property name="title" translatable="yes">Registry editor</property>
                            <property name="subtitle" translatable="yes">Edit the internal registry.</property>
                            <child>
                              <object class="GtkBox">
                                <property name="spacing">10</property>
                                <child>
                                  <object class="GtkButton" id="btn_regedit">
                                    <property name="halign">center</property>
                                    <property name="valign">center</property>
                                    <child>
                                      <object class="GtkImage">
                                        <property name="icon-name">system-run-symbolic</property>
                                      </object>
                                    </child>
                                    <style>
                                      <class name="btn_undecorated"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow">
                            <property name="title" translatable="yes">Debug</property>
                            <property name="subtitle" translatable="yes">Debug wine processes.</property>
                            <child>
                              <object class="GtkBox">
                                <property name="spacing">10</property>
                                <child>
                                  <object class="GtkButton" id="btn_debug">
                                    <property name="halign">center</property>
                                    <property name="valign">center</property>
                                    <child>
                                      <object class="GtkImage">
                                        <property name="icon-name">system-run-symbolic</property>
                                      </object>
                                    </child>
                                    <style>
                                      <class name="btn_undecorated"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="btn_help_debug">
                                    <property name="tooltip-text" translatable="yes">Read documentation</property>
                                    <property name="halign">end</property>
                                    <property name="valign">center</property>
                                    <child>
                                      <object class="GtkImage">
                                        <property name="icon-name">dialog-information-symbolic</property>
                                      </object>
                                    </child>
                                    <style>
                                      <class name="image-button"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow">
                            <property name="title" translatable="yes">Task manager</property>
                            <property name="subtitle" translatable="yes">Manage processes with the Wine task manager.</property>
                            <child>
                              <object class="GtkBox">
                                <property name="spacing">10</property>
                                <child>
                                  <object class="GtkButton" id="btn_taskmanager">
                                    <property name="halign">center</property>
                                    <property name="valign">center</property>
                                    <child>
                                      <object class="GtkImage">
                                        <property name="icon-name">system-run-symbolic</property>
                                      </object>
                                    </child>
                                    <style>
                                      <class name="btn_undecorated"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow">
                            <property name="title" translatable="yes">Wine config</property>
                            <property name="subtitle" translatable="yes">Adjust internal settings.</property>
                            <child>
                              <object class="GtkBox">
                                <property name="spacing">10</property>
                                <child>
                                  <object class="GtkButton" id="btn_winecfg">
                                    <property name="halign">center</property>
                                    <property name="valign">center</property>
                                    <child>
                                      <object class="GtkImage">
                                        <property name="icon-name">system-run-symbolic</property>
                                      </object>
                                    </child>
                                    <style>
                                      <class name="btn_undecorated"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow" id="row_uninstaller">
                            <property name="title" translatable="yes">Uninstaller</property>
                            <property name="subtitle" translatable="yes">Uninstall programs using wine uninstaller</property>
                            <child>
                              <object class="GtkBox">
                                <property name="spacing">10</property>
                                <child>
                                  <object class="GtkButton" id="btn_uninstaller">
                                    <property name="halign">center</property>
                                    <property name="valign">center</property>
                                    <child>
                                      <object class="GtkImage">
                                        <property name="icon-name">system-run-symbolic</property>
                                      </object>
                                    </child>
                                    <style>
                                      <class name="btn_undecorated"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow">
                            <property name="title" translatable="yes">Control panel</property>
                            <property name="subtitle" translatable="yes">Access the internal wine control panel.</property>
                            <child>
                              <object class="GtkBox">
                                <property name="spacing">10</property>
                                <child>
                                  <object class="GtkButton" id="btn_controlpanel">
                                    <property name="halign">center</property>
                                    <property name="valign">center</property>
                                    <child>
                                      <object class="GtkImage">
                                        <property name="icon-name">system-run-symbolic</property>
                                      </object>
                                    </child>
                                    <style>
                                      <class name="btn_undecorated"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <placeholder/>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
  <object class="HdyActionRow" id="row_no_programs">
    <property name="visible">True</property>
    <property name="can-focus">True</property>
    <child>
      <object class="GtkBox">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <property name="hexpand">True</property>
        <property name="vexpand">True</property>
        <child>
          <object class="GtkLabel">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
            <property name="label" translatable="yes">Use the Installers section or the "Run executable" button above.</property>
            <property name="wrap">True</property>
            <style>
              <class name="dim-label"/>
            </style>
          </object>
          <packing>
            <property name="expand">True</property>
            <property name="fill">True</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
