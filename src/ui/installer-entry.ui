<?xml version="1.0" encoding="UTF-8"?>
<interface>
<requires lib="gtk" version="4.0"/>
  <requires lib="libhandy" version="0.0"/>
  <object class="GtkPopover" id="pop_actions">
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">3</property>
        <child>
          <object class="GtkModelButton" id="btn_manifest">
            
            <property name="text" translatable="yes">Show manifest</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_report">
            
            <property name="text" translatable="yes">Bug report</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_review">
            
            <property name="text" translatable="yes">Read review</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkSeparator">
          </object>
          
          
        </child>
      </object>
    </child>
  </object>
  <template class="InstallerEntry" parent="AdwActionRow">
    
    <property name="title">Installer name</property>
    <property name="subtitle">Installer description</property>
    <child>
      <object class="GtkBox">
        <property name="spacing">10</property>
        <child>
          <object class="GtkMenuButton">
            
            <property name="focus-on-click">False</property>
            <property name="halign">center</property>
            <property name="valign">center</property>
            <property name="popover">pop_actions</property>
            <child>
              <object class="GtkImage">
                <property name="icon-name">view-more-symbolic</property>
              </object>
            </child>
            <style>
              <class name="image-button"/>
            </style>
          </object>
          
          
        </child>
        <child>
          <object class="GtkSeparator">
          </object>
          
          
        </child>
        <child>
          <object class="GtkButton" id="btn_install">
            
            <property name="halign">center</property>
            <property name="valign">center</property>
            <child>
              <object class="GtkImage">
                <property name="icon-name">document-save-symbolic</property>
              </object>
            </child>
            <style>
              <class name="image-button"/>
            </style>
          </object>
          
          
        </child>
        <child>
          <object class="GtkImage" id="img_error">
            <property name="icon-name">dialog-warning-symbolic</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkImage" id="img_installed">
            <property name="icon-name">object-select-symbolic</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkLabel" id="label_grade">
            <property name="halign">center</property>
            <property name="valign">center</property>
            <property name="label">Unknown</property>
            <attributes>
              <attribute name="font-desc" value="Cantarell 10"/>
              <attribute name="weight" value="normal"/>
            </attributes>
            <style>
              <class name="tag"/>
            </style>
          </object>
          
          
        </child>
        <child>
          <object class="GtkLabel" id="label_step">
          </object>
          
          
        </child>
      </object>
    </child>
  </template>
</interface>
