<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.24"/>
  <object class="GtkPopover" id="pop_context">
    <property name="can-focus">False</property>
    <child>
      <object class="GtkBox">
        <property name="width-request">160</property>
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <property name="border-width">9</property>
        <property name="orientation">vertical</property>
        <property name="spacing">3</property>
        <child>
          <object class="GtkModelButton" id="btn_remove">
            <property name="visible">True</property>
            <property name="can-focus">True</property>
            <property name="receives-default">False</property>
            <property name="tooltip-text" translatable="yes">Read documentation about installers	</property>
            <property name="text" translatable="yes">Remove from library</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">3</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
  <template class="LibraryEntry" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkImage" id="img_cover">
        <property name="icon_size">6</property>
      </object>
      
      
    </child>
    <child>
      <object class="GtkBox">
        <property name="spacing">5</property>
        <child>
          <object class="GtkMenuButton">
            
            <property name="focus-on-click">False</property>
            <property name="valign">center</property>
            <property name="popover">pop_context</property>
            <child>
              <object class="GtkImage">
                <property name="icon-name">view-more-symbolic</property>
              </object>
            </child>
          </object>
          
          
        </child>
        <child>
          <object class="GtkImage" id="img_icon">
            <property name="can-focus">False</property>
            <property name="icon-name">bottles-steam-symbolic</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkLabel" id="label_name">
                <property name="halign">start</property>
                <property name="label">Item name</property>
                <attributes>
                  <attribute name="weight" value="semibold"/>
                  <attribute name="scale" value="1.1000000000000001"/>
                </attributes>
              </object>
              
              
            </child>
            <child>
              <object class="GtkLabel" id="label_bottle">
                <property name="halign">start</property>
                <property name="label">Bottle name</property>
                <attributes>
                  <attribute name="scale" value="0.90000000000000002"/>
                </attributes>
              </object>
              
              
            </child>
          </object>
          
          
        </child>
        <child>
          <object class="GtkBox">
            <property name="spacing">10</property>
            <child>
              <object class="GtkButton" id="btn_launch_steam">
                
                <property name="tooltip-text" translatable="yes">Launch with Steam</property>
                <property name="halign">center</property>
                <property name="valign">center</property>
                <child>
                  <object class="GtkImage">
                    <property name="icon-name">bottles-steam-symbolic</property>
                  </object>
                </child>
                <style>
                  <class name="image-button"/>
                </style>
              </object>
              
              
            </child>
            <child>
              <object class="GtkButton" id="btn_run">
                
                <property name="halign">center</property>
                <property name="valign">center</property>
                <child>
                  <object class="GtkImage">
                    <property name="icon-name">media-playback-start-symbolic</property>
                  </object>
                </child>
                <style>
                  <class name="image-button"/>
                </style>
              </object>
              
              
            </child>
            <child>
              <placeholder/>
            </child>
            <child>
              <object class="GtkButton" id="btn_stop">
                
                <property name="halign">center</property>
                <property name="valign">center</property>
                <child>
                  <object class="GtkImage">
                    <property name="icon-name">media-playback-stop-symbolic</property>
                  </object>
                </child>
                <style>
                  <class name="image-button"/>
                </style>
              </object>
              
              
            </child>
          </object>
          
          
        </child>
        <style>
          <class name="library-entry-details"/>
        </style>
      </object>
      
      
    </child>
    <style>
      <class name="library-entry"/>
    </style>
  </template>
  <object class="GtkPopover" id="pop_context">
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">3</property>
        <child>
          <object class="GtkModelButton" id="btn_remove">
            
            <property name="tooltip-text" translatable="yes">Read documentation about installers	</property>
            <property name="text" translatable="yes">Remove from library</property>
          </object>
          
          
        </child>
      </object>
    </child>
  </object>
</interface>
