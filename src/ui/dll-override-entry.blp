using Gtk 4.0;
using Adw 1;

template DLLEntry : Adw.ComboRow {
  title: "DLL Name";
  model:
  StringList {
    items [
      _("Builtin (Wine)"),
      _("Native (Windows)"),
      _("Builtin, then Native"),
      _("Native, then Builtin"),
      _("Disabled"),
    ]
  }

  ;

  Button btn_remove {
    valign: center;
    icon-name: "user-trash-symbolic";
    tooltip-text: _("Remove");

    styles [
      "flat",
    ]
  }
}
