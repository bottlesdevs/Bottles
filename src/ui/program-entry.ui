<?xml version="1.0" encoding="UTF-8"?>
<interface>
<requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <object class="GtkPopover" id="pop_actions">
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">3</property>
        <child>
          <object class="GtkModelButton" id="btn_launch_terminal">
            
            <property name="text" translatable="yes">Launch with terminal</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_launch_options">
            
            <property name="text" translatable="yes">Change launch options</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_browse">
            
            <property name="text" translatable="yes">Browse path</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_rename">
            
            <property name="text" translatable="yes">Rename</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkSeparator">
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_winehq">
            
            <property name="text" translatable="yes">WineHQ</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_protondb">
            
            <property name="text" translatable="yes">ProtonDB</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_forum">
            
            <property name="text" translatable="yes">Bottles' Forums</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkSeparator">
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_add_entry">
            
            <property name="text" translatable="yes">Add desktop entry</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_add_library">
            
            <property name="text" translatable="yes">Add to my library</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_hide">
            
            <property name="text" translatable="yes">Hide this program</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_unhide">
            
            <property name="text" translatable="yes">Unhide this program</property>
          </object>
          
          
        </child>
        <child>
          <object class="GtkModelButton" id="btn_uninstall">
            
            <property name="text" translatable="yes">Uninstall</property>
          </object>
          
          
        </child>
      </object>
    </child>
  </object>
  <template class="ProgramEntry" parent="AdwActionRow">
    
    <property name="title">Program name</property>
    <child>
      <object class="GtkBox">
        <property name="spacing">10</property>
        <child>
          <object class="GtkButton" id="btn_launch_steam">
            
            <property name="tooltip-text" translatable="yes">Launch with Steam</property>
            <property name="halign">center</property>
            <property name="valign">center</property>
            <child>
              <object class="GtkImage">
                <property name="icon-name">bottles-steam-symbolic</property>
              </object>
            </child>
            <style>
              <class name="image-button"/>
            </style>
          </object>
          
          
        </child>
        <child>
          <object class="GtkMenuButton" id="btn_menu">
            
            <property name="focus-on-click">False</property>
            <property name="halign">center</property>
            <property name="valign">center</property>
            <property name="popover">pop_actions</property>
            <child>
              <object class="GtkImage">
                <property name="icon-name">view-more-symbolic</property>
              </object>
            </child>
            <style>
              <class name="image-button"/>
            </style>
          </object>
          
          
        </child>
        <child>
          <object class="GtkButton" id="btn_run">
            
            <property name="halign">center</property>
            <property name="valign">center</property>
            <child>
              <object class="GtkImage">
                <property name="icon-name">media-playback-start-symbolic</property>
              </object>
            </child>
            <style>
              <class name="image-button"/>
            </style>
          </object>
          
          
        </child>
        <child>
          <object class="GtkButton" id="btn_stop">
            
            <property name="halign">center</property>
            <property name="valign">center</property>
            <child>
              <object class="GtkImage">
                <property name="icon-name">media-playback-stop-symbolic</property>
              </object>
            </child>
            <style>
              <class name="image-button"/>
            </style>
          </object>
          
          
        </child>
        <child>
          <object class="GtkSeparator" id="sep">
          </object>
          
          
        </child>
      </object>
    </child>
  </template>
</interface>
